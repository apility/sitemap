{
    "name": "netflex/sitemap",
    "description": "Laravelium Sitemap package for Laravel.",
    "homepage": "https://laravelium.com",
    "keywords": [
        "laravelium",
        "laravel",
        "php",
        "sitemap",
        "generator",
        "xml",
        "html",
        "google-news"
    ],
    "license": "MIT",
    "replace": {
        "laravelium/sitemap": "8.*"
    },
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no",
            "role": "Developer",
            "homepage": "https://apility.no"
        },
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/netflex/sitemap/issues",
        "source": "https://github.com/netflex/sitemap",
        "wiki": "https://github.com/netflex/sitemap/wiki"
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^9.0",
        "illuminate/filesystem": "^9.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench-core": "^6.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Laravelium\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}